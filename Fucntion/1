#include <iostream>
#include <string>
void famName() { std ::cout << "Hello from global scope!" << std ::endl; }

void myFunction(std ::string fname) {

  std ::cout << fname << " Tep " << std ::endl;
} // this is a paramater& argument
void country(char grade = 'B') {
  std ::cout << grade << std ::endl;
} // this is call default paramater because we give the value to it
void info(std ::string name, int age) {
  std ::cout << name << " Tep " << age << " Year old. " << std ::endl;
} // this is the multi paramater
int main() {
  famName();            // this is just a normal one like for void
  myFunction("Navuth"); // this is a paramater&argument
  myFunction("Theavy");
  myFunction("Tepy");
  myFunction("Chenda");
  country(); // this two are the default paramater
  country('D');
  info(" Chenda ", 18);

  return 0;
}
